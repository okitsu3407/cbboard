<p id="notice"><%= notice %></p>

<h1>Meyasus</h1>

<table>
  <thead>
    <tr>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meyasus.each do |meyasu| %>
      <tr>
        <td><%= meyasu.content %></td>
        <td><%= link_to 'Show', meyasu %></td>
        <td><%= link_to 'Edit', edit_meyasu_path(meyasu) %></td>
        <td><%= link_to 'Destroy', meyasu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meyasu', new_meyasu_path %>
